generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ProjectRequests {
  id        Int      @id @default(autoincrement())
  name      String
  email     String
  company   String?
  phone     String
  message   String
  budget    String
  createdAt DateTime @default(now())
}
